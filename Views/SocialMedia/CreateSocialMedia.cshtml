@model SocialMedia
@{
    ViewData["Title"] = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Sosyal Medya Ekleme Sayfası</h2>

<br />
<form method="post">
    <label>Url</label>
    <input type="text" asp-for="SocialMediaUrl" class="form-control" style="border:1px solid black;" />
    <br />

    <label>Icon</label>
    <input type="text" asp-for="SocialMediaIcon" class="form-control" style="border:1px solid black;" />
    <br />
   
    <button class="btn btn-primary">Kaydet</button>
</form>

