@model List<Education>

<div class="col-lg-6" data-aos="fade-up" data-aos-delay="200">
    <h3 class="resume-title">Eğitim Hayatım</h3>
    <br />
    @foreach (var item in Model)
    {
        <div class="resume-item">
            <h4>@item.Title</h4>
            <h5>@item.DateInfo</h5>
            <p><em>@item.SubTitle</em></p>
            <ul>
                @item.Description
            </ul>
        </div>
    }
</div>
